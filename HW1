#include<stdio.h>
#include<string.h>

int main() {
    char input[1000];
    scanf("%s", input);

    int length = strlen(input);

    for (int i = 0; i < length; i += 2) {
        char character = input[i];
        int count = input[i + 1] - '0';

        for (int j = 0; j < count; j++) {
            printf("%c", character);
        }
    }

    printf("\n");

    return 0;
}
