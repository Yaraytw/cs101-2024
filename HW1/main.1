#include <stdio.h>

int main() {
    char input[101];
    
    scanf("%s", input);

    // 遍歷字串並計算出現次數
    int count = 1;
    for (int i = 0; input[i] != '\0'; i++) {
        if (input[i] == input[i + 1]) {
            count++;
        } else {
            printf("%c%d", input[i], count);
            count = 1;
        }
    }

    return 0;
}
