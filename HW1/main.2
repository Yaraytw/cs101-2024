//將字元與其出現次數轉換成字串
//Description
//反轉 "HW1_1" 的輸出與輸入
//Input
//輸入(scanf)字元與其出現次數
//Output
//輸出 (printf) 轉換後的字串
//Sample Input 1 
//A2B3C4d3\n
//Sample Output 1
//AABBBCCCCddd\n
//Sample Input 2 
//A1B1C1D1\n
//Sample Output 2
//ABCD\n
#include <stdio.h>

int main() {
    char input[101];
   
    scanf("%s", input);

    // 遍歷輸入字串，反轉並輸出
    for (int i = 0; input[i] != '\0'; i += 2) {
        char character = input[i];
        int count = input[i + 1] - '0';

        for (int j = 0; j < count; j++) {
            printf("%c", character);
        }
    }

    return 0;
}
